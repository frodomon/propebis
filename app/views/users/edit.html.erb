<% provide(:title, "Propebis | Editar Usuario") %>
<div class="row wrapper border-bottom white-bg page-heading breadcrumbs-margin">
  <div class="col-lg-12">
    <h2>Editar Usuario</h2>
    <ol class="breadcrumb">
      <li>
        <a href="<%= url_for :controller => 'static_pages', :action => 'home' %>">Inicio</a>
      </li>
      <li>
        <a href="<%= url_for :controller => 'users', :action => 'index' %>">Usuarios</a>
      </li>
      <li class="active">
        <strong>Editar Usuario</strong>
      </li>
    </ol>
  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight no-padding">
  <%= render 'shared/notifications' %>
  <div class="row breadcrumbs-margin">
  	<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
      <div class="ibox float-e-margins">
     	  <div class="ibox-title">
    	    <h3>Editar Usuario</h3>
    	  </div>
    	  <div class="ibox-content">
    	    <%= render 'form' %>
    		  <%= link_to 'Volver', :back, class: 'btn btn-w-m btn-info' %>
    	  </div>
      </div>
	</div>
  </div>	
</div>